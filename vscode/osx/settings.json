// Place your settings in this file to overwrite the default settings
{
    "editor.fontFamily": "Fira Code, 'Operator Mono', 'OperatorMono Nerd Font', Menlo, Monaco, 'Courier New', monospace",
    "editor.tabSize": 2,
    "editor.wordWrapColumn": 800,
    "editor.useTabStops": false,
    "editor.renderWhitespace": "all",
    "editor.fontSize": 12,
    "editor.lineHeight": 24,
    "editor.tabCompletion": "on",
    "editor.fontLigatures": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.autoIndent": false,
    "editor.fontWeight": "400",
    "editor.quickSuggestionsDelay": 5,
    "editor.formatOnPaste": false,
    "workbench.colorTheme": "New Moon",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorCustomizations": {},
    "workbench.sideBar.location": "left",
    "workbench.panel.defaultLocation": "right",
    "workbench.activityBar.visible": true,
    "[php]": {
        "editor.tabSize": 4
    },
    "files.encoding": "utf8",
    "files.associations": {
        "*.vue": "vue",
        "*.gs": "javascript"
    },
	"files.autoSave": "off",
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        ".git": true,
        "**/.tmp": true,
        "**/node_modules": true,
        "**/*.zip": true,
        "**/*.tar": true,
        "**/*.gz": true,
        "**/*.nyc_output": true,
        "**/.sass-cache": true
    },
    "html.format.wrapLineLength": 160,
    "html.format.endWithNewline": true,
    "extensions.autoUpdate": true,
    "telemetry.enableTelemetry": false,
    "php-cs-fixer.onsave": false,
    "html.format.indentHandlebars": true,
    "emmet.triggerExpansionOnTab": true,
    "emmet.preferences": {
        "filter.commentAfter": "\n<!-- /[#ID][.CLASS] -->"
    },
    "emmet.includeLanguages": {
        "blade": "html",
        "html": "html",
        "erb": "html",
        "edge": "html",
        "javascript": "javascriptreact",
        "vue": "html",
        "gs": "javascript",
        "vue-html": "html",
        "ejs": "html"
    },
    "emmet.syntaxProfiles": {
        "javascript": "javascriptreact",
        "sass": "css",
        "gs": "javascript",
        "vue-html": "html",
        "vue": "html",
        "erb": "html"
    },
    "search.location": "panel",
    "search.useIgnoreFiles": true,
    "search.useGlobalIgnoreFiles": false,
    "search.collapseResults": "alwaysCollapse",
    "search.exclude": {
        "build": true,
        ".temp": true,
        "temp": true,
        ".tmp": true,
        "tmp": true,
        "**/*.min.*": true,
        "**/bower": true,
        "**/.tmp": true,
        "**/_bower": true,
        "**/_site": true,
        "**/dev": true,
        "**/local": true,
        "**/prod": true,
        "**/.nuxt": true,
        "**/dist": true,
        "**/wp-admin": true,
        "**/wp-includes": true,
        "**/node_modules": true,
        "**/.sass-cache": true
    },
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "trailing-spaces.trimOnSave": true,
    "window.zoomLevel": 1,
    "material-icon-theme.showUpdateMessage": false,
    "typescript.check.npmIsInstalled": false,
    "extensions.ignoreRecommendations": true,
    "php-cs-fixer.executablePath": "/usr/local/bin/php-cs-fixer",
    "breadcrumbs.enabled": false,
    "eslint.autoFixOnSave": false,
    "eslint.alwaysShowStatus": false,
    "eslint.enable": false,
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "vue"
    ],
    "scss.validate": false,
    "css.validate": false,
    "less.validate": false,
    "javascript.preferences.quoteStyle": "single",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.suggest.completeJSDocs": true,
    "javascript.format.enable": false,
    "javascript.autoClosingTags": true,
    "editor.insertSpaces": false,
    "prettier.stylelintIntegration": true,
    "stylelint.enable": false,
    "px2rem-plus.comments": true,
    "px2rem-plus.base": 16,
    "px2rem-plus.leadingZero": true,
    "[scss]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "terminal.integrated.rendererType": "dom",
    "yaml.schemas": {
        "file:///Users/oseunando/.vscode/extensions/atlassian.atlascode-1.2.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "team.showWelcomeMessage": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "terminal.integrated.shell.osx": "/bin/zsh"
}