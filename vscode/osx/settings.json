// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "Fira Code, FiraFlott, 'Operator Mono', 'OperatorMono Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 800,
  "editor.useTabStops": false,
  "editor.renderWhitespace": "all",
  "editor.fontSize": 11,
  "editor.lineHeight": 22,
  "editor.tabCompletion": "on",
  "editor.fontLigatures": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.autoIndent": "none",
  "editor.fontWeight": "500",
  "editor.quickSuggestionsDelay": 5,
  "editor.formatOnPaste": false,
  "workbench.colorTheme": "New Moon",
  "workbench.sideBar.location": "left",
  "workbench.panel.defaultLocation": "right",
  "workbench.activityBar.visible": true,
  "[php]": {
    "editor.tabSize": 4
  },
  "files.encoding": "utf8",
  "files.associations": {
    "*.vue": "vue",
    "*.gs": "javascript",
    ".stylelintrc": "json",
    "*.mdx": "mdx"
  },
  "files.autoSave": "off",
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    ".git": true,
    "**/.sass-cache": true,
    "**/.tmp": true,
    "**/*.gz": true,
    "**/*.nyc_output": true,
    "**/*.tar": true,
    "**/*.zip": true,
    "**/node_modules": true,
    "api/**/*": true,
    "node_modules/**/*": true
  },
  "html.format.wrapLineLength": 160,
  "html.format.endWithNewline": true,
  "extensions.autoUpdate": true,
  "telemetry.enableTelemetry": false,
  "html.format.indentHandlebars": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.preferences": {
    "filter.commentAfter": "\n<!-- /[#ID][.CLASS] -->"
  },
  "emmet.includeLanguages": {
    "blade": "html",
    "html": "html",
    "erb": "html",
    "edge": "html",
    "ts": "javascript",
    "javascript": "javascriptreact",
    "vue": "html",
    "php": "html",
    "gs": "javascript",
    "vue-html": "html",
    "ejs": "html"
  },
  "emmet.syntaxProfiles": {
    "javascript": "javascriptreact",
    "sass": "css",
    "gs": "javascript",
    "vue-html": "html",
    "vue": "html",
    "php": "html",
    "erb": "html"
  },
  "search.useIgnoreFiles": true,
  "search.useGlobalIgnoreFiles": false,
  "search.exclude": {
    "**/build": true,
    ".temp": true,
    "temp": true,
    ".tmp": true,
    "tmp": true,
    "**/*.min.*": true,
    "**/bower": true,
    "**/.tmp": true,
    "**/_bower": true,
    "**/_site": true,
    "**/dev": true,
    "**/local": true,
    "**/prod": true,
    "**/.nuxt": true,
    "**/dist": true,
    "**/wp-admin": true,
    "**/wp-includes": true,
    "**/node_modules": true,
    "**/.sass-cache": true
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "trailing-spaces.trimOnSave": true,
  "window.zoomLevel": 1,
  "material-icon-theme.showUpdateMessage": false,
  "typescript.check.npmIsInstalled": false,
  "extensions.ignoreRecommendations": true,
  "php-cs-fixer.executablePath": "/usr/local/bin/php-cs-fixer",
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "ts",
    "typescript",
    "javascriptreact",
    "vue"
  ],
  "scss.validate": false,
  "css.validate": false,
  "less.validate": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.completeJSDocs": true,
  "javascript.format.enable": false,
  "javascript.autoClosingTags": true,
  "editor.insertSpaces": false,
  "stylelint.enable": false,
  "px2rem-plus.base": 16,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.rendererType": "dom",
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "files.eol": "\n",
  "npm.packageManager": "yarn",
  "npm.bin": "yarn",
  "eslint.packageManager": "yarn",
  "git.ignoreMissingGitWarning": true,
  "eslint.enable": false,
  "px2rem-plus.comments": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.startupEditor": "newUntitledFile",
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  },
  "eslint.migration.2_x": "off",
  "prettier.ignorePath": ".gitignore,.prettierignore",
  "material-icon-theme.activeIconPack": "vue_vuex",
  "liveServer.settings.donotShowInfoMsg": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.fontAliasing": "antialiased",
  "turboConsoleLog.quote": "'",
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "turboConsoleLog.wrapLogMessage": false,
  "turboConsoleLog.logMessagePrefix": "[  LOG  ] ",
  "workbench.iconTheme": "material-icon-theme"
}