{
  "Create arrow function": {
    "prefix": "afunc",
    "body": [
      "($1) => {",
      "  $2",
      "}"
    ],
    "description": "Create arrow function"
  },
  "Node module import from": {
    "prefix": "ifrom",
    "body": [
      "import ${Module} from '${module}'"
    ],
    "description": "Node module import from"
  },
  "Node module require": {
    "prefix": "require",
    "body": [
      "${const} ${Module} = require('${module}');"
    ],
    "description": "Node module require"
  },
  "Create a executable function": {
    "prefix": "exfunc",
    "body": [
      "(function(${$}) {",
      "  $3",
      "})(${jQuery});"
    ],
    "description": "auto executable function"
  },
  "const function": {
    "prefix": "cfunc",
    "body": [
      "const ${Var} = () => {",
      "  return {$0}",
      "}"
    ],
    "description": "const function"
  },
  "Create a jQuery app function": {
    "prefix": "jfunc",
    "body": [
      "var app = app || {};",
      "",
      "app.general = (function (${$}) {",
      "  'use strict';",
      "",
      "  var config = {};",
      "",
      "  function init () {",
      "    console.info('[init]', config);",
      "  }",
      "",
      "  return {",
      "    init: init",
      "  };",
      "})(${jQuery});",
      ""
    ],
    "description": "jQuery app function"
  },
  "Print to console": {
    "prefix": "clog",
    "body": [
      "console.${2:log}(${1:var})"
    ],
    "description": "Log output to console"
  },
  "Export default": {
    "prefix": "edef",
    "body": [
      "export default {",
      "  $0",
      "}"
    ],
    "description": "export default"
  },
  "GAS Logger.log": {
    "prefix": "logger",
    "body": [
      "Logger.log($0)"
    ],
    "description": "GAS Logger.log"
  },
  "Vue Logger": {
    "prefix": "vlogger",
    "body": [
      "Vue.$${2:log}.${3:error}(${4:'${5:error}',}$1)"
    ],
    "description": "Vue Logger"
  },
  "Vuex start": {
    "prefix": "vx",
    "body": [
      "const state = {",
      "  isLoading: ${1:false},",
      "}",
      "",
      "const mutations = {",
      "  'TOGGLE_LOADING' (state, payload = false) {",
      "    state.isLoading = !!payload",
      "  }",
      "}",
      "",
      "const actions = {",
      "  toggleLoading ({ commit }, isLoading = false) {",
      "    commit('TOGGLE_LOADING', isLoading)",
      "  }",
      "}",
      "",
      "export default {",
      "  namespaced: true,",
      "  state,",
      "  mutations,",
      "  actions",
      "}"
    ],
    "description": "Vuex start"
  }
}